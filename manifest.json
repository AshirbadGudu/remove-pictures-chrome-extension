{
  "manifest_version": 2,
  "name": "demo-ext",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_title": "Demo Extension"
  }
}
